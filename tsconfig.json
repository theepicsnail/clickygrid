{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "classic",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "seedrandom"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "compileOnSave": true
}
